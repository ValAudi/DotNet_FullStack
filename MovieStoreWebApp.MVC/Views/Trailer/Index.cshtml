@{

}

@Model Trailers


<table class="table">
  <thead class="table-dark">
      <tr>
      <th scope="col">#</th>
      <th scope="col">@Model.Id</th>
      <th scope="col">@Model.TrailerName</th>
      <th scope="col">@Model.TrailerUrl</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Model.Id</td>
            <td>@Model.TrailerName</td>
            <td>@Model.TrailerUrl</td>
        </tr>
    }
  </tbody>
</table>
